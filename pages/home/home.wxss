.floorshow {
  flex: 0 0 386rpx;
  display: flex;
  position: relative;
}
.floorshow image{
  border-radius: 5rpx;
}
.floorshow image:nth-child(1) {
  flex: 0 0 254rpx;
  height: 386rpx;
}

.floorshow image:nth-child(2) {
  width: 233rpx;
  height: 188rpx;
  position: absolute;
  right: 250rpx;
}

.floorshow image:nth-child(3) {
  width: 233rpx;
  height: 188rpx;
  position: absolute;
  right: 0;
}

.floorshow image:nth-child(4) {
  width: 233rpx;
  height: 188rpx;
  position: absolute;
  right: 250rpx;
  bottom: 0;

}

.floorshow image:nth-child(5) {
  width: 233rpx;
  height: 188rpx;
  position: absolute;
  right: 0;
  bottom: 0;

}
